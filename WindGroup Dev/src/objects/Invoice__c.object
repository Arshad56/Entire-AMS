    <webLinks>
        <fullName>Refund</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Refund</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/37.0/connection.js&apos;)}

var totalOutS = 0.0;
var tenant = String(&apos;{!Invoice__c.Tenant__c}&apos;);
console.log(tenant);
if(tenant!= null || tenant!= &apos;&apos;){
var result = sforce.connection.query(&quot;Select Name, Id, Outstanding_Amount__c,Billing_Type__c from Invoice__c where Tenant_Name__c = \&apos;{!Invoice__c.Tenant__c}\&apos; and Status__c != \&apos;Cancelled\&apos; and Billing_Type__c != \&apos;Credit Note\&apos;&quot;);
var records = result.getArray(&quot;records&quot;);
  for (var i=0; i&lt; records.length; i++) {
    var record = records[i];
    totalOutS += parseFloat(record.Outstanding_Amount__c);
    console.log(record.Id+&quot; Name==&gt; &quot;+record.Name + &quot; --ve-&gt; &quot; + record.Outstanding_Amount__c + &quot; ## &quot;+(-1)*record.Outstanding_Amount__c);
    console.log(totalOutS);
  }
}

var balance = parseFloat(&apos;{!Invoice__c.Amount__c}&apos;.replace(&apos;$&apos;,&apos;&apos;).replace(&apos;,&apos;,&apos;&apos;)) + parseFloat(&apos;{!Invoice__c.Amount_Paid__c}&apos;.replace(&apos;$&apos;,&apos;&apos;).replace(&apos;,&apos;,&apos;&apos;));


var reqRefAmount = parseFloat(&apos;{!Invoice__c.Total_Refunded_Amount__c}&apos;.replace(&apos;$&apos;,&apos;&apos;).replace(&apos;,&apos;,&apos;&apos;));


var outStanding = parseFloat(&apos;{!Invoice__c.Outstanding_Amount__c}&apos;.replace(&apos;$&apos;,&apos;&apos;).replace(&apos;,&apos;,&apos;&apos;));

//alert(reqRefAmount + &quot; -- &quot;+outStanding +&quot; -- &quot;+(-1)*outStanding);

if( &apos;{! Invoice__c.Billing_Type__c}&apos; != &apos;Credit Note&apos;){ 
alert(&quot;Only Credit Note Invoices can be refunded.&quot;); 
}else if( &apos;{!Invoice__c.Status__c}&apos; == &apos;Cancelled&apos;){ 
alert(&quot;The Credit Note is already cancelled and can&apos;t be refunded.&quot;); 
}else if({!Invoice__c.Voided__c}){ 
alert(&quot;The Credit Note is already voided and can&apos;t be refunded.&quot;); 
}else if(&apos;{!Invoice__c.Refund_Status__c}&apos; == &apos;Submitted&apos;){
alert(&quot;This Invoice record is locked for approval.&quot;); 
}else if( outStanding &gt;= 0 ){
alert(&quot;There is no balance for this Credit Note to refund.&quot;); 
}else if(totalOutS &gt; 0){
	if(&apos;{!$Profile.Name}&apos; == &apos;System Administrator&apos;){
		var r = confirm(&quot;There are outstanding payment for the tenant. \nDo you still want to proceed to refund?&quot;);
		if (r == true) {
		   window.location=&apos;/apex/InvoiceRefund?id={!Invoice__c.Id}&apos;;
		}
	}else{
		alert(&quot;There are outstanding payments and Credit Note can\&apos;t be refunded for the Tenant.&quot;); 
	}

}else if(outStanding &gt;= reqRefAmount){

alert(&apos;There is no available balance to be refunded&apos;);
}else{ 
window.location=&apos;/apex/InvoiceRefund?id={!Invoice__c.Id}&apos;;

}</url>
    </webLinks>
