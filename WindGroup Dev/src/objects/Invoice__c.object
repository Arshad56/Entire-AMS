    <fields>
        <fullName>JV_Type__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( CASE (Billing_Type__c,
	   &quot;Late Charge&quot;, &quot;1&quot;,
           &quot;Ad-Hoc Invoice&quot;, &quot;1&quot;,
           &quot;Monthly&quot;, &quot;1&quot;,
           &quot;Miscellaneous&quot;, &quot;1&quot;,
&quot;GTO Rent&quot;,&quot;1&quot;,
           &quot;Debit Note&quot;,&quot;1&quot;,
           &quot;0&quot; ) == &quot;1&quot; , &quot;Invoice (Normal)&quot;, 
IF(Billing_Type__c == &quot;Credit Note&quot;, &quot;Credit Note&quot;, 
   IF(Billing_Type__c == &quot;Late Payment&quot;, &quot;Late Payment&quot;, 
    IF(Billing_Type__c == &quot;Rent&quot;, &quot;Invoice (Advance Rental)&quot;, &quot;&quot;) )))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This is used for Journal Voucher process. Used to sync/integrate data with JE Logic and configuration objects.</inlineHelpText>
        <label>JV Type</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
