    <fields>
        <fullName>Late_Charge_Days__c</fullName>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBlank(Last_Payment_Date__c)),
IF((Last_Payment_Date__c - Payment_Due_Date__c) &gt; 0,(Last_Payment_Date__c - Payment_Due_Date__c),0)
,IF((TODAY() - Payment_Due_Date__c) &gt; 0,(TODAY() - Payment_Due_Date__c),0)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Late Charge Days</label>
