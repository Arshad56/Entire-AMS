{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)}
if((&apos;{!Invoice__c.Voided__c}&apos; == &apos;1&apos;)){ 
   alert(&apos;This Invoice is already Voided.&apos;);
}else{
if((&apos;{!Invoice__c.Amount_Paid__c}&apos;==&apos;$0.00&apos; || &apos;{!Invoice__c.Amount_Paid__c}&apos;==&apos;&apos;) &amp;&amp; (&apos;{!Invoice__c.JV_Status__c}&apos; != &apos;Generated&apos; &amp;&amp; &apos;{!Invoice__c.JV_Status__c}&apos; != &apos;Posted&apos;)){
	
		var r = confirm(&quot;Are you sure you want to Void this Invoice?&quot;);

		if(r == true){
			var retVal; 
			while (retVal == null || retVal == &apos;&apos;) {
				retVal = prompt(&quot;Please Enter Void Reason* (mandatory):&quot;, &quot;&quot;);
			  if(retVal === null){
				break;
			  }
			}
			if(retVal === null){
				
			}else{
				sforce.apex.execute(&quot;InvoiceButtonClick&quot;,&quot;voidAllInvoices&quot;,{invoiceId:&quot;{!Invoice__c.Id}&quot;,reason:retVal});
				location.reload(true);
			}
		}
	
}else if(!(&apos;{!Invoice__c.Amount_Paid__c}&apos;==&apos;$0.00&apos; || &apos;{!Invoice__c.Amount_Paid__c}&apos;==&apos;&apos;)){
	alert(&quot;Warning : Paid Invoices cannot be Voided.&quot;);
}else if(&apos;{!Invoice__c.JV_Status__c}&apos;==&apos;Generated&apos; || &apos;{!Invoice__c.JV_Status__c}&apos;==&apos;Posted&apos;){
	alert(&quot;Warning : Generated/Posted Invoices cannot be Voided.&quot;);
}
}</url>
