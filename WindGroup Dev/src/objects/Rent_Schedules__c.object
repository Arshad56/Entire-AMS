IF ( Total_Electric_Units__c &gt; 0 &amp;&amp; ( ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;Gov Meter&quot;) || ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;Sub Meter&quot;)),Total_Electric_Units__c  ,
IF ( Total_Electric_Units__c &gt; 0 &amp;&amp;  ISPICKVAL(Lease_Charge__r.Charge__r.Charge_Type__c, &quot;Generator&quot;) &amp;&amp;
ISPICKVAL(Lease_Charge__r.Charge__r.Type__c, &quot;Meter Units&quot;)      ,Total_Electric_Units__c  ,
IF ( Total_Phone_Units__c &gt; 0 &amp;&amp; (ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;Local&quot;) || ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;Trunk&quot;))  ,Total_Phone_Units__c,
IF ( GTO_Monthly_Total_Converted__c &gt; 0 &amp;&amp; ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;GTO Rent&quot;)  &amp;&amp;  Tiered__c,  
GTO_Monthly_Total_Converted__c ,
0))))))</formula>
