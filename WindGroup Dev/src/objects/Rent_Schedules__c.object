        <externalId>false</externalId>
        <formula>if( IsPickVal(Lease_Charge__r.Type__c, &quot;Meter Units&quot;)  , 1 , Calc_No_of_Days_Pro_Rated_2__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Managed by Trigger</inlineHelpText>
        <label>Calc - No of Days (Pro-Rated)</label>
        <precision>18</precision>
