    <fields>
        <fullName>Calculated_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL( Lease_Charge__r.Charge_Type__c , &quot;GTO Rent&quot;),
If( ISPICKVAL(Lease_Unit__r.Lease_Agreement_No__r.GTO_Billed__c , &quot;By Lease (Summary)&quot;)  
|| ISPICKVAL(Lease_Unit__r.Lease_Agreement_No__r.GTO_Billed__c , &quot;By Lease (Lease Unit)&quot;) ,  
GTO_Rent_By_Lease__c ,  
Calculated_Amount_Higher_or_Sum__c  ) 
, 
IF( ISPICKVAL(Lease_Charge__r.Type__c, &quot;Fixed&quot;) , 
	Lease_Charge__r.Charge_Rate__c , 

IF(ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;Advance Rental&quot;)  &amp;&amp; ISPICKVAL(Lease_Charge__r.Billing_Frequency__c, &quot;Monthly&quot;) , 
Lease_Charge__r.Charge_Rate__c * Lease_Charge__r.Lease_Unit__r.Area_Sq_Ft__c * 1 ,

IF(ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;Advance Rental&quot;)  &amp;&amp; ISPICKVAL(Lease_Charge__r.Billing_Frequency__c, &quot;Quarterly&quot;) , 
Lease_Charge__r.Charge_Rate__c * Lease_Charge__r.Lease_Unit__r.Area_Sq_Ft__c * 3 , 

IF(ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;Advance Rental&quot;)  &amp;&amp; ISPICKVAL(Lease_Charge__r.Billing_Frequency__c, &quot;Half-Yearly&quot;) , 
Lease_Charge__r.Charge_Rate__c * Lease_Charge__r.Lease_Unit__r.Area_Sq_Ft__c * 6 , 

IF(ISPICKVAL(Lease_Charge__r.Charge_Type__c, &quot;Advance Rental&quot;)  &amp;&amp; ISPICKVAL(Lease_Charge__r.Billing_Frequency__c, &quot;Annual&quot;) , 
Lease_Charge__r.Charge_Rate__c * Lease_Charge__r.Lease_Unit__r.Area_Sq_Ft__c * 12 , 

IF( ISPICKVAL(Lease_Charge__r.Type__c, &quot;By Area&quot;) , 
	Lease_Charge__r.Charge_Rate__c  * Lease_Charge__r.Lease_Unit__r.Area_Sq_Ft__c , 

IF( ISPICKVAL(Lease_Charge__r.Type__c, &quot;By Percent&quot;) ,	
	(Lease_Charge__r.Charge_Rate__c /100 ) * Converted_Base_Rent__c, 

IF( ISPICKVAL(Lease_Charge__r.Type__c, &quot;Meter Units&quot;) , 

	Lease_Charge__r.Charge_Rate__c * Total_Units_p__c , 
	0
)))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calculated Amount</label>
        <precision>18</precision>
