    <fields>
        <fullName>Calc_NonCumulative_Tiered__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Total_Units_p__c&gt; Lease_Charge__r.Tier_8_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_8_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_9_Rate__c * Total_Units_p__c , 

IF( Total_Units_p__c&gt; Lease_Charge__r.Tier_7_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_7_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_8_Rate__c * Total_Units_p__c, 

IF( Total_Units_p__c&gt; Lease_Charge__r.Tier_6_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_6_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_7_Rate__c * Total_Units_p__c, 

IF( Total_Units_p__c&gt; Lease_Charge__r.Tier_5_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_5_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_6_Rate__c * Total_Units_p__c, 

IF( Total_Units_p__c&gt; Lease_Charge__r.Tier_4_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_4_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_5_Rate__c * Total_Units_p__c, 

IF( Total_Units_p__c&gt; Lease_Charge__r.Tier_3_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_3_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_4_Rate__c * Total_Units_p__c, 

IF( Total_Units_p__c&gt; Lease_Charge__r.Tier_2_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_2_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_3_Rate__c * Total_Units_p__c , 

IF( Total_Units_p__c&gt; Lease_Charge__r.Tier_1_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_1_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_2_Rate__c * Total_Units_p__c, 
Lease_Charge__r.Tier_1_Rate__c * Total_Units_p__c))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Shows Calculations only when &apos;Tiered&apos; is checked!</inlineHelpText>
        <label>Calc - Non Cumulative Tiered</label>
        <precision>18</precision>
        <required>false</required>
