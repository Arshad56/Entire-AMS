    <fields>
        <fullName>Est_Calc_Cumulative_Tiered__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Total_Estimated_GTO_Converted__c &gt; Lease_Charge__r.Tier_5_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_5_Upper_Limit__c &gt; 0, 
( Lease_Charge__r.Tier_6_Rate__c * (Total_Estimated_GTO_Converted__c - Lease_Charge__r.Tier_5_Upper_Limit__c )) + 
( Lease_Charge__r.Tier_5_Rate__c * (Lease_Charge__r.Tier_5_Upper_Limit__c -Lease_Charge__r.Tier_4_Upper_Limit__c)) + 
( Lease_Charge__r.Tier_4_Rate__c * (Lease_Charge__r.Tier_4_Upper_Limit__c -Lease_Charge__r.Tier_3_Upper_Limit__c)) + 
( Lease_Charge__r.Tier_3_Rate__c * (Lease_Charge__r.Tier_3_Upper_Limit__c -Lease_Charge__r.Tier_2_Upper_Limit__c)) + 
( Lease_Charge__r.Tier_2_Rate__c * (Lease_Charge__r.Tier_2_Upper_Limit__c - Lease_Charge__r.Tier_1_Upper_Limit__c)) + 
( Lease_Charge__r.Tier_1_Rate__c * (Lease_Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Tiered_Lower_Limit__c )), 

IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_4_Upper_Limit__c &amp;&amp;
Lease_Charge__r.Tier_4_Upper_Limit__c &gt; 0,
( Lease_Charge__r.Tier_5_Rate__c * (Total_Estimated_GTO_Converted__c - Lease_Charge__r.Tier_4_Upper_Limit__c )) + 
( Lease_Charge__r.Tier_4_Rate__c * (Lease_Charge__r.Tier_4_Upper_Limit__c - Lease_Charge__r.Tier_3_Upper_Limit__c )) + 
( Lease_Charge__r.Tier_3_Rate__c * (Lease_Charge__r.Tier_3_Upper_Limit__c -Lease_Charge__r.Tier_2_Upper_Limit__c)) + 
( Lease_Charge__r.Tier_2_Rate__c * (Lease_Charge__r.Tier_2_Upper_Limit__c -Lease_Charge__r.Tier_1_Upper_Limit__c))+ 
( Lease_Charge__r.Tier_1_Rate__c * ( Lease_Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Tiered_Lower_Limit__c )), 

IF( Total_Estimated_GTO_Converted__c &gt; Lease_Charge__r.Tier_3_Upper_Limit__c &amp;&amp;
Lease_Charge__r.Tier_3_Upper_Limit__c &gt; 0,
( Lease_Charge__r.Tier_4_Rate__c * ( Total_Estimated_GTO_Converted__c- Lease_Charge__r.Tier_3_Upper_Limit__c )) + 
( Lease_Charge__r.Tier_3_Rate__c * (Lease_Charge__r.Tier_3_Upper_Limit__c - Lease_Charge__r.Tier_2_Upper_Limit__c )) + 
( Lease_Charge__r.Tier_2_Rate__c * (Lease_Charge__r.Tier_2_Upper_Limit__c - Lease_Charge__r.Tier_1_Upper_Limit__c)) + 
( Lease_Charge__r.Tier_1_Rate__c * (Lease_Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Tiered_Lower_Limit__c )), 

IF( Total_Estimated_GTO_Converted__c &gt; Lease_Charge__r.Tier_2_Upper_Limit__c &amp;&amp;
Lease_Charge__r.Tier_2_Upper_Limit__c &gt; 0,
( Lease_Charge__r.Tier_3_Rate__c * (Total_Estimated_GTO_Converted__c- Lease_Charge__r.Tier_2_Upper_Limit__c )) + 
( Lease_Charge__r.Tier_2_Rate__c * (Lease_Charge__r.Tier_2_Upper_Limit__c - Lease_Charge__r.Tier_1_Upper_Limit__c )) + 
( Lease_Charge__r.Tier_1_Rate__c * (Lease_Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Tiered_Lower_Limit__c )), 

IF( Total_Estimated_GTO_Converted__c &gt; Lease_Charge__r.Tier_1_Upper_Limit__c &amp;&amp;
Lease_Charge__r.Tier_1_Upper_Limit__c &gt; 0,
( Lease_Charge__r.Tier_2_Rate__c * (Total_Estimated_GTO_Converted__c- Lease_Charge__r.Tier_1_Upper_Limit__c )) + 
( Lease_Charge__r.Tier_1_Rate__c * (Lease_Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Tiered_Lower_Limit__c )) , 
Lease_Charge__r.Tier_1_Rate__c * Total_Estimated_GTO_Converted__c )))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>look into using function of fields instead of this</inlineHelpText>
        <label>Est Calc - Cumulative - Tiered</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Est_Calc_Non_Cumulative_Tiered__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_8_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_8_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_9_Rate__c * Total_Estimated_GTO_Converted__c, 

IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_7_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_7_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_8_Rate__c * Total_Estimated_GTO_Converted__c, 

IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_6_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_6_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_7_Rate__c * Total_Estimated_GTO_Converted__c, 

IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_5_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_5_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_6_Rate__c * Total_Estimated_GTO_Converted__c, 

IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_4_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_4_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_5_Rate__c * Total_Estimated_GTO_Converted__c, 

IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_3_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_3_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_4_Rate__c * Total_Estimated_GTO_Converted__c, 

IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_2_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_2_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_3_Rate__c * Total_Estimated_GTO_Converted__c, 

IF( Total_Estimated_GTO_Converted__c&gt; Lease_Charge__r.Tier_1_Upper_Limit__c &amp;&amp; Lease_Charge__r.Tier_1_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Tier_2_Rate__c * Total_Estimated_GTO_Converted__c, 
Lease_Charge__r.Tier_1_Rate__c * Total_Estimated_GTO_Converted__c))))))))</formula>
        <inlineHelpText>look into using function of fields instead of this</inlineHelpText>
        <label>Est Calc - Non Cumulative Tiered</label>
        <precision>18</precision>
        <required>false</required>
