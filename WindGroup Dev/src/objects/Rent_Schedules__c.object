    <fields>
        <fullName>Terminated_Month_cal__c</fullName>
        <externalId>false</externalId>
        <formula>If(TEXT( Lease_Charge__r.Billing_Terms__c) = &quot;Non-prorate&quot; &amp;&amp; Terminated_Month__c &amp;&amp; MONTH(Start_Date__c) &lt;&gt; MONTH(End_Date__c)
,

IF( MONTH(Start_Date__c)  == 2 &amp;&amp; Leap_Year__c, 
(( 29 -  DAY(Start_Date__c) ) + 1 ) / 29,
IF( MONTH(Start_Date__c)  == 2 &amp;&amp; NOT(Leap_Year__c), 
(( 28 -  DAY(Start_Date__c)) + 1 ) / 28,
IF( MONTH(Start_Date__c) == 1 || MONTH(Start_Date__c) == 3 ||MONTH(Start_Date__c) == 5 ||MONTH(Start_Date__c) == 7 ||MONTH(Start_Date__c) == 12 ||MONTH(Start_Date__c) == 8||MONTH(Start_Date__c) == 10  ,
(( 31 -  DAY(Start_Date__c)) + 1 )/ 31,
IF(MONTH(Start_Date__c) == 4 || MONTH(Start_Date__c) == 6 ||MONTH(Start_Date__c) == 11||MONTH(Start_Date__c) == 9 ,
(( 30 -  DAY(Start_Date__c)) + 1 )/ 30,
1))))
+
IF( MONTH(Start_Date__c)  == 2 &amp;&amp; Leap_Year__c, 
DAY(End_Date__c)  / 29,
IF( MONTH(Start_Date__c)  == 2 &amp;&amp; NOT(Leap_Year__c), 
 DAY(End_Date__c)/ 28,
IF( MONTH(Start_Date__c) == 1 || MONTH(Start_Date__c) == 3 ||MONTH(Start_Date__c) == 5 ||MONTH(Start_Date__c) == 7 ||MONTH(Start_Date__c) == 12 ||MONTH(Start_Date__c) == 8||MONTH(Start_Date__c) == 10  ,
DAY(End_Date__c)/ 31,
IF(MONTH(Start_Date__c) == 4 || MONTH(Start_Date__c) == 6 ||MONTH(Start_Date__c) == 11||MONTH(Start_Date__c) == 9 ,
DAY(End_Date__c)/ 30,
1))))
,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Terminated Month %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
