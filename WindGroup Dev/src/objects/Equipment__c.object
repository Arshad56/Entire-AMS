    <webLinks>
        <fullName>Return</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Return</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/apex.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

if(&quot;{!Equipment__c.Action__c}&quot;==&quot;Install&quot; || &quot;{!Equipment__c.Action__c}&quot;==&quot;Issue&quot;){ 
var left = Number((screen.width/2)-220); 
var tops = Number((screen.height/2)-250); 
var win=window.open(&apos;/apex/EquipmentReturn?Id={!Equipment__c.Id}&apos;,&apos;&apos;, 
&apos;width=500,height=350,scrollbars=yes,top=&apos;+tops+&apos;,left=&apos;+left); 
}else{ 
alert(&apos;Only installed or issued equipments can be returned.&apos;); 
}</url>
    </webLinks>
    <webLinks>
        <fullName>Transfer</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Transfer</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/apex.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

if(&quot;{!Inventory__c.Product_Type__c}&quot;==&quot;Equipment&quot;){

	var records = {!GETRECORDIDS($ObjectType.Equipment__c)}.toString().replace(/,/g , &quot;&apos;,&apos;&quot;);
	if(records.length&lt;1) {
		alert(&quot;Please choose at least one Equipment.&quot;);
	}else{
		var equipment = sforce.connection.query(&quot;SELECT id,Fixed_Asset_ID__c, Action__c, Status__c from Equipment__c where id in (&apos;&quot;+records+&quot;&apos;)&quot;);
		var equipRecords = equipment.getArray(&quot;records&quot;);
		var error = false;
		var ids = &apos;&apos;;
		
		for(var i=0;i&lt;equipRecords.length;i++){
			if(equipRecords[i].Fixed_Asset_ID__c==null){
				alert(&apos;Equipment(s) Fixed Asset ID should not be blank.&apos;);
				error = true;
				break;
			} else if (equipRecords[i].Action__c ==&apos;Issue&apos; || equipRecords[i].Action__c ==&apos;Write Off&apos; || equipRecords[i].Action__c ==&apos;Install&apos;){
				alert(&apos;Equipment(s) should be in the Inventory&apos;);
				error = true;
				break;
			} else if (equipRecords[i].Status__c ==&apos;Inactive&apos;){
				alert(&apos;Equipment(s) should be Active Equipment.&apos;);
				error = true;
				break;
			}
			ids += equipRecords[i].Id+&apos;,&apos;;
		}

		if(error==false){
			var left = Number((screen.width/2)-220); 
			var tops = Number((screen.height/2)-250); 
			var win=window.open(&apos;/apex/EquipmentTransfer?Ids=&apos;+ids,&apos;&apos;, 
			&apos;width=500,height=350,scrollbars=yes,top=&apos;+tops+&apos;,left=&apos;+left);
		}
	
	}
}else{
	alert(&quot;Transfer can be done for Equipment Type Inventory only.&quot;); 
}</url>
    </webLinks>
