        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/apex.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

if(!{!ISBLANK(Equipment__c.Fixed_Asset_ID__c)}){ 
if(&quot;{!Equipment__c.Status__c}&quot;==&quot;Active&quot;){ 
var left = Number((screen.width/2)-220); 
var tops = Number((screen.height/2)-250); 
var win=window.open(&apos;/apex/EquipmentInstall?Id={!Equipment__c.Id}&apos;,&apos;&apos;, 
&apos;width=500,height=350,scrollbars=yes,top=&apos;+tops+&apos;,left=&apos;+left); 
}else{ 
alert(&apos;Equipment should be Active to issue.&apos;); 
} 
}else{ 
alert(&apos;Fixed Asset ID should not be blank.&apos;); 
}</url>
