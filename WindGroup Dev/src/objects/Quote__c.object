    <validationRules>
        <fullName>Update_only_for_particular_PO_status</fullName>
        <active>true</active>
        <description>Quote is readonly if PO Status is Inapproval or Issued</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(PO_No__r.Status__c,&apos;Issued&apos;),ISPICKVAL(PO_No__r.Status__c,&apos;In-Approval&apos;)),NOT(ISNEW()))</errorConditionFormula>
        <errorMessage>Quote can only be updated if PO Status is &apos;Draft&apos; or &apos;Rejected&apos;</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>New_Quote</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Quote</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>if(&quot;{!Purchase_Order__c.Status__c}&quot; != &quot;Draft&quot; &amp;&amp; &quot;{!Purchase_Order__c.Status__c}&quot; != &quot;Rejected&quot; ){
alert(&quot;PO status must be Draft or Rejected to create a new record&quot;);
}
else{
window.location.href=&quot;/a28/e?CF00NO0000002oQiH={!Purchase_Order__c.Name}&amp;CF00NO0000002oQiH_lkid={!Purchase_Order__c.Id}&amp;retURL={!Purchase_Order__c.Id}&quot;;
}</url>
    </webLinks>
