{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

if(&quot;{!Item_Catalogue__c.Status__c}&quot;==&quot;Active&quot;){
if(&quot;{!Item_Catalogue__c.Product_Type__c}&quot;==&quot;Consumable&quot;  || &quot;{!Item_Catalogue__c.Product_Type__c}&quot;==&quot;Equipment&quot;){
var left = Number((screen.width/2)-220); 
var tops = Number((screen.height/2)-250); 
var win=window.open(&apos;/apex/ProductReplace?Id={!Item_Catalogue__c.Id}&apos;,&apos;&apos;, 
&apos;width=500,height=350,scrollbars=yes,top=&apos;+tops+&apos;,left=&apos;+left);
}else{
alert(&quot;Only consumable or Equipment type of product can be replaced.&quot;);
}
}else{
alert(&quot;Only active product can be replaced.&quot;);
}</url>
