    <webLinks>
        <fullName>Generate_PO_PDF</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Generate PO PDF</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)}
if(&quot;{!Purchase_Order__c.Status__c}&quot; != &apos;Issued&apos;){
alert(&quot;Status should be &apos;Issued&apos;&quot;);
}
else{
if({!Purchase_Order__c.Generate_PO_PDF__c}){
alert(&quot;Purchase Order is already Generated&quot;);
}else{
var p = new sforce.SObject(&apos;Purchase_Order__c&apos;);
p.id = &quot;{!Purchase_Order__c.Id}&quot;;
p.Generate_PO_PDF__c = &quot;true&quot;;
result= sforce.connection.update([p]);
window.location.href=&quot;/{!Purchase_Order__c.Id}&quot;;
}
}</url>
    </webLinks>
    <webLinks>
        <fullName>Generate_Payment_Arrangement</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Generate Payment Arrangement</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)}
if(&quot;{!Purchase_Order__c.Vendor_Name__c}&quot;){
if(&quot;{!Purchase_Order__c.Status__c}&quot; == &quot;Draft&quot;  ){
var check = confirm(&quot;Are you sure?&quot;);
if(check){
sforce.apex.execute(&quot;CreatePaymentAssigment&quot;,&quot;createRecord&quot;,{purOrderId:&quot;{!Purchase_Order__c.Id}&quot;});

location.reload();
}
}
else
alert(&quot;Status should be Draft&quot;);
}
else
alert(&quot;Please select Vendor Name&quot;);</url>
