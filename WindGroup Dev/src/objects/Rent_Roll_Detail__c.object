    <fields>
        <fullName>Calc_Cumulative_Tiered__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Total_Units_p__c &gt; Lease_Charge__r.Charge__r.Tier_5_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_5_Upper_Limit__c &gt; 0, 
( Lease_Charge__r.Charge__r.Tier_6_Rate__c * (Total_Units_p__c - Lease_Charge__r.Charge__r.Tier_5_Upper_Limit__c + Lease_Charge__r.Charge__r.Tier_4_Upper_Limit__c + Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c +  Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c + Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c)  +
( Lease_Charge__r.Charge__r.Tier_5_Rate__c * Lease_Charge__r.Charge__r.Tier_5_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_4_Rate__c * Lease_Charge__r.Charge__r.Tier_4_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_3_Rate__c * Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_2_Rate__c * Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_1_Rate__c * ( Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Charge__r.Tiered_Lower_Limit__c ))),
IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_4_Upper_Limit__c , 
( Lease_Charge__r.Charge__r.Tier_5_Rate__c * (Total_Units_p__c - Lease_Charge__r.Charge__r.Tier_4_Upper_Limit__c + Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c +   Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c + Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_4_Rate__c * Lease_Charge__r.Charge__r.Tier_4_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_3_Rate__c * Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_2_Rate__c * Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_1_Rate__c * ( Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Charge__r.Tiered_Lower_Limit__c ))),
IF( Total_Units_p__c &gt; Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c , 
( Lease_Charge__r.Charge__r.Tier_4_Rate__c * ( Total_Units_p__c- Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c + Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c +  Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_3_Rate__c * Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_2_Rate__c * Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_1_Rate__c * ( Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Charge__r.Tiered_Lower_Limit__c ))),
IF( Total_Units_p__c &gt; Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c , 
( Lease_Charge__r.Charge__r.Tier_3_Rate__c * ( Total_Units_p__c- ( Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c + Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c) ) ) +
( Lease_Charge__r.Charge__r.Tier_2_Rate__c * Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c ) +
( Lease_Charge__r.Charge__r.Tier_1_Rate__c * ( Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Charge__r.Tiered_Lower_Limit__c )),
IF( Total_Units_p__c &gt; Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c , 
( Lease_Charge__r.Charge__r.Tier_2_Rate__c * ( Total_Units_p__c - Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c )) +
( Lease_Charge__r.Charge__r.Tier_1_Rate__c * ( Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c - Lease_Charge__r.Charge__r.Tiered_Lower_Limit__c )) , 
Lease_Charge__r.Charge__r.Tier_1_Rate__c * Total_Units_p__c )))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calc - Cumulative - Tiered</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Calc_No_of_Days_Pro_Rated__c</fullName>
        <externalId>false</externalId>
        <formula>IF((CONTAINS( UPPER( Lease_Charge__r.Name ) , &quot;ADVANCE&quot;) &amp;&amp; NOT  ISPICKVAL(Lease_Charge__r.Billing_Frequency__c, &quot;Monthly&quot;) ) || 
 ISPICKVAL(Lease_Charge__r.Type__c, &quot;Meter Units&quot;)  , 
1, 
IF(Billing_Month__c == 2, 
(( End_Date__c -  Start_Date__c ) + 1 ) / 28,
IF( Billing_Month__c == 1 || Billing_Month__c == 3 ||Billing_Month__c == 5 ||Billing_Month__c == 7 ||Billing_Month__c == 12 ||Billing_Month__c == 8||Billing_Month__c == 10  ,
(( End_Date__c -  Start_Date__c ) + 1 )/ 31,
IF(Billing_Month__c == 4 || Billing_Month__c == 6 ||Billing_Month__c == 11||Billing_Month__c == 9 ,
(( End_Date__c -  Start_Date__c ) + 1 )/ 30,
1))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calc - No of Days (Pro-Rated)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Calc_Non_Cumulative_Tiered__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_8_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_8_Upper_Limit__c &gt; 0  , 
Lease_Charge__r.Charge__r.Tier_8_Rate__c * Total_Units_p__c / 100,
IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_7_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_7_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Charge__r.Tier_7_Rate__c * Total_Units_p__c / 100,
IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_6_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_6_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Charge__r.Tier_6_Rate__c * Total_Units_p__c / 100,
IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_5_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_5_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Charge__r.Tier_5_Rate__c * Total_Units_p__c / 100,
IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_4_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_4_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Charge__r.Tier_4_Rate__c * Total_Units_p__c / 100,
IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_3_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Charge__r.Tier_3_Rate__c * Total_Units_p__c / 100,
IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_2_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Charge__r.Tier_2_Rate__c * Total_Units_p__c / 100 ,
IF( Total_Units_p__c&gt; Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c &amp;&amp; Lease_Charge__r.Charge__r.Tier_1_Upper_Limit__c &gt; 0 , 
Lease_Charge__r.Charge__r.Tier_1_Rate__c * Total_Units_p__c / 100,
Lease_Charge__r.Charge__r.Tiered_Lower_Limit__c * Total_Units_p__c / 100))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calc - Non Cumulative Tiered</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
