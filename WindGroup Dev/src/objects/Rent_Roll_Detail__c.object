        <formula>IF ( ( CONTAINS( UPPER( Lease_Charge__r.Name ) , &quot;WATER&quot;) || CONTAINS( UPPER( Lease_Charge__r.Name ) , &quot;GAS&quot; )) &amp;&amp; NOT( Lease_Charge__r.Charge__r.Tiered_Limit__c  ) &amp;&amp; 
