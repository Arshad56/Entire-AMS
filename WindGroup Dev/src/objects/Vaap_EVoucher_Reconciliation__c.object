    <fields>
        <fullName>Ready_to_Reconcile__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
TEXT(Payment_Mode__c)= TEXT(WireCard_Payment_Mode__c) ,  
Transaction_Amount__c =  WireCard_Amount__c ,   
TEXT(Currency_Code__c) = TEXT(WireCard_Currency_Code__c),
Transaction_Amount__c &gt;=0,
WireCard_Amount__c&gt;=0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ready to Reconcile</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
