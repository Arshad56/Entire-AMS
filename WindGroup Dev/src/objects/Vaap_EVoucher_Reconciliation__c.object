    <validationRules>
        <fullName>Vaap_Validate_Vaap_and_WireCard_Data</fullName>
        <active>true</active>
        <description>This is to check if Vaap and Wirecard data are matched before update on Reconciled Date.</description>
        <errorConditionFormula>AND( 
NOT(Ready_to_Reconcile__c),  
NOT(ISBLANK(Reconciled_Date__c))
)</errorConditionFormula>
        <errorMessage>Vaap and WireCard data are not matched. Please review and amend before Reconcile.</errorMessage>
    </validationRules>
