    <fields>
        <fullName>Expected_Charge__c</fullName>
        <externalId>false</externalId>
        <formula>IF(CONTAINS( UPPER( Charge__r.Name ) , &quot;ADVANCE&quot;) &amp;&amp;  ISPICKVAL(Billing_Frequency__c, &quot;Monthly&quot;) ,   Charge_Rate__c * Lease_Unit__r.Area_Sq_Ft__c  * 1  ,
IF(CONTAINS( UPPER( Charge__r.Name ) , &quot;ADVANCE&quot;) &amp;&amp;  ISPICKVAL(Billing_Frequency__c, &quot;Quarterly&quot;) ,   Charge_Rate__c * Lease_Unit__r.Area_Sq_Ft__c  * 3  ,
IF(CONTAINS( UPPER( Charge__r.Name ) , &quot;ADVANCE&quot;) &amp;&amp;  ISPICKVAL(Billing_Frequency__c, &quot;Half-yearly&quot;) ,   Charge_Rate__c * Lease_Unit__r.Area_Sq_Ft__c  * 6 ,
IF(CONTAINS( UPPER( Charge__r.Name ) , &quot;ADVANCE&quot;) &amp;&amp;  ISPICKVAL(Billing_Frequency__c, &quot;Yearly&quot;) ,   Charge_Rate__c * Lease_Unit__r.Area_Sq_Ft__c  * 12 ,
IF( ISPICKVAL(Type__c, &quot;GTO (Sum)&quot;),    MAX(Lease_Unit__r.Current_Base_Rent__c,(Expected_GTO__c * Charge_Rate__c ))   ,
IF( ISPICKVAL(Type__c, &quot;GTO (Higher)&quot;) ,   Lease_Unit__r.Current_Base_Rent__c + (Expected_GTO__c * Charge_Rate__c )  ,
IF( ISPICKVAL(Type__c, &quot;Fixed&quot;) ,   Charge_Rate__c  , 
IF( ISPICKVAL(Type__c, &quot;By Area&quot;) ,   Charge_Rate__c * 	Lease_Unit__r.Area_Sq_Ft__c  , 
IF( ISPICKVAL(Type__c, &quot;By Percent&quot;) ,(Charge_Rate__c / 100) * Lease_Unit__r.Current_Base_Rent__c  ,
0)))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Expected Charge (Rent / Taxations) (to b</label>
