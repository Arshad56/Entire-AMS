    <fields>
        <fullName>Base_Rent_Calculation__c</fullName>
        <description>This field is referred in Rent Schedule - &quot;Calculated Amount&quot; for Amount Calculation
**Do not delete**</description>
        <externalId>false</externalId>
        <formula>IF
(
	AND (
	OR(
	ISPICKVAL ( Charge_Type__c , &quot;Rent&quot;) ,
	ISPICKVAL ( Charge_Type__c , &quot;Advance Rental&quot;) ) ,
	 ISPICKVAL(Type__c, &quot;Fixed&quot;)),
	Charge_Rate__c ,
	IF
(
	AND (
	OR(
	ISPICKVAL ( Charge_Type__c , &quot;Rent&quot;) ,
	ISPICKVAL ( Charge_Type__c , &quot;Advance Rental&quot;) ) ,
	 ISPICKVAL(Type__c, &quot;By Area&quot;)),
	Charge_Rate__c * Lease_Unit__r.Area_Sq_Ft__c,
0))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If Type  - Area then &apos;Charge Rate * Unit Area&apos;
If Type - Fixed then &apos;Charge Rate&apos;</inlineHelpText>
        <label>Base Rent Calculation</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
