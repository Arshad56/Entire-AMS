}else if(&apos;{!Lease_Charges__c.Pre_Termination__c}&apos; == true){ 
alert(&apos;This Lease Charge is already Terminated.&apos;); 
}else{ 
var today = new Date();
var parts =&apos;{!Lease_Charges__c.End_Date__c}&apos;.split(&apos;/&apos;);
