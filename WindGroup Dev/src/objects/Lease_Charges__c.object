    <webLinks>
        <fullName>Refresh_Rent_Schedules</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Refresh Rent Schedules</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/connection.js&quot;)} 
try 
{ 
var strLCQuery = &quot;Select id,Commercial_Tax_Apply__c,Lease_Unit__r.Lease_Agreement_No__r.Property__r.Comm_Tax__c, GTO_Cap__c,Cap_Applicable__c from Lease_Charges__c where id=&apos;&quot;+&apos;{!Lease_Charges__c.Id}&apos;+&quot;&apos;&quot;; 

var otherLC = sforce.connection.query(strLCQuery); 
var lcRecords = otherLC.getArray(&quot;records&quot;); 

var strQuery=&quot;Select Id,Name from Rent_Schedules__c where Lease_Charge__r.Id =&apos;&quot; +&apos;{!Lease_Charges__c.Id}&apos;+&quot;&apos; And Status__c = &apos;In-Tenancy&apos;&quot;; 

var newRecords = []; 

var otherP = sforce.connection.query(strQuery); 
var records = otherP.getArray(&quot;records&quot;); 

//alert(otherP); 
for(var i=0; i &lt; records.length ; i ++) 
{ 
var ob=new sforce.SObject(&quot;Rent_Schedules__c&quot;); 
ob.Id=records[i].Id; 
if(lcRecords[0].Commercial_Tax_Apply__c){
if(lcRecords[0].Lease_Unit__r.Lease_Agreement_No__r.Property__r.Comm_Tax__c!=null){
ob.Comm_Tax__c = lcRecords[0].Lease_Unit__r.Lease_Agreement_No__r.Property__r.Comm_Tax__c;
}else{
ob.Comm_Tax__c = 5;
}
}
ob.Cap_Applicable__c = lcRecords[0].Cap_Applicable__c;
ob.Cap_Value__c = lcRecords[0].GTO_Cap__c;

newRecords.push(ob); 
} 
var result=sforce.connection.update(newRecords); 
//alert(result); 
location.reload(); 
}catch(er) 
{ 
alert(er); 
}</url>
    </webLinks>
