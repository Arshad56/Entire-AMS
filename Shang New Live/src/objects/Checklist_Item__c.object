        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Close Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)}

if(&apos;{!Request__c.Status__c}&apos; == &apos;Completed&apos;){
alert(&apos;This Request has been already marked as Completed!&apos;);
}else{
var r = confirm(&quot;Are you sure you want to Close this Request?&quot;);
if(r == true){
    var reqObj = new sforce.SObject(&quot;Request__c&quot;); 
    reqObj.Id = &apos;{!Request__c.Id}&apos;; 
    reqObj.Status__c = &apos;Completed&apos;;
    var now     = new Date();
    var currentTime = now.getDate()+&apos;/&apos;+ (now.getMonth()+1) +&apos;/&apos;+ now.getFullYear() +&apos; &apos;+ now.getHours() +&apos;:&apos;+  now.getMinutes();
    reqObj.Date_Time_Closed__c = new Date().toISOString();
    var result = sforce.connection.update([reqObj]);	
    location.reload(true);
}
}</url>
